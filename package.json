{
  "name": "three",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@ant-design/pro-table": "2.55.0",
    "@xkool/api": "4.6.67",
    "@xkool/core": "4.6.60",
    "@xkool/dt": "4.6.91",
    "@xkool/ui": "4.6.74",
    "@xkool/utils": "4.6.32",
    "@xkool/graphic": "4.6.37",
    "alife-logger": "1.8.20",
    "antd": "4.20.2",
    "axios": "0.21.1",
    "classnames": "2.2.6",
    "crypto-browserify": "^3.12.0",
    "dayjs": "1.10.7",
    "echarts": "4.6.0",
    "echarts-for-react": "3.0.1",
    "fabric": "5.2.1-browser",
    "geojson": "0.5.0",
    "html2canvas": "1.4.1",
    "immer": "9.0.6",
    "jsts": "2.7.2",
    "jszip-utils": "0.1.0",
    "lodash": "4.17.21",
    "pako": "1.0.11",
    "pubsub-js": "1.9.3",
    "qrcode.react": "3.0.1",
    "qs": "6.9.1",
    "query-string": "7.1.1",
    "rbush": "3.0.1",
    "react": "18.1.0",
    "react-color": "2.19.3",
    "react-copy-to-clipboard": "5.1.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.1.0",
    "react-draggable": "4.4.5",
    "react-hot-keys": "2.7.2",
    "react-infinite-scroller": "^1.2.4",
    "react-intl": "5.25.1",
    "react-redux": "8.0.1",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.0",
    "react-uid": "2.3.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.9",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "three": "0.126.1"
  },
  "scripts": {
    "start": "cross-env REACT_APP_STAGE=local PORT=3500 craco start",
    "build": "npm run build:local",
    "build:local": "cross-env REACT_APP_STAGE=local craco build",
    "build:dev": "cross-env REACT_APP_STAGE=dev craco --max_old_space_size=8192 build",
    "build:test": "cross-env REACT_APP_STAGE=test craco --max_old_space_size=8192 build",
    "build:rc": "cross-env REACT_APP_STAGE=rc craco --max_old_space_size=8192 build",
    "build:hotfix": "cross-env REACT_APP_STAGE=hotfix craco --max_old_space_size=8192 build",
    "build:prod": "cross-env GENERATE_SOURCEMAP=false REACT_APP_STAGE=prod craco --max_old_space_size=8192 build",
    "test:coverage": "cross-env CI=true craco test --coverage",
    "test": "craco test",
    "eject": "react-scripts eject",
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js' --html result.html",
    "lint": "npm run lint:script && npm run lint:style && npm run lint:prettier",
    "lint:script": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint \"src/**/*.less\"",
    "lint:prettier": "prettier -c \"src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}\"",
    "lint:fix": "npm run lint:fix-script && npm run lint:fix-style",
    "lint:fix-script": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:fix-style": "stylelint --fix \"src/**/*.less\"",
    "prettier": "prettier -c --write \"**/*\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "install:package": "npm i @xkool/api @xkool/utils @xkool/ui @xkool/core @xkool/dt --save-exact --legacy-peer-deps"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "browser": {
    "buffer": false
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@craco/craco": "7.0.0-alpha.0",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "5.1.0",
    "@testing-library/user-event": "7.1.2",
    "@types/classnames": "2.2.9",
    "@types/echarts": "4.4.2",
    "@types/fabric": "4.5.7",
    "@types/geojson": "7946.0.7",
    "@types/jest": "27.0.3",
    "@types/jsts": "0.17.12",
    "@types/lodash": "4.14.170",
    "@types/node": "17.0.31",
    "@types/qrcode.react": "1.0.2",
    "@types/qs": "6.9.0",
    "@types/react": "18.0.9",
    "@types/react-color": "3.0.4",
    "@types/react-copy-to-clipboard": "5.0.2",
    "@types/react-dom": "18.0.3",
    "@types/react-infinite-scroller": "^1.2.2",
    "@types/react-redux": "7.1.24",
    "@types/three": "0.126.1",
    "@typescript-eslint/eslint-plugin": "5.24.0",
    "@typescript-eslint/parser": "5.24.0",
    "antd-dayjs-webpack-plugin": "1.0.6",
    "babel-jest": "27.5.1",
    "babel-plugin-import": "1.13.3",
    "buffer": "^6.0.3",
    "craco-less": "2.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-formatter-pretty": "4.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.2.2",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-unicorn": "42.0.0",
    "http-proxy-middleware": "2.0.6",
    "husky": "8.0.1",
    "jest-canvas-mock": "2.2.0",
    "jest-json-reporter2": "1.1.0",
    "less": "3.10.3",
    "less-loader": "10.0.0",
    "lint-staged": "12.4.1",
    "postcss": "8.4.6",
    "postcss-less": "6.0.0",
    "prettier": "2.6.2",
    "react-test-renderer": "18.1.0",
    "slash": "3.0.0",
    "source-map-explorer": "2.1.2",
    "stream-browserify": "^3.0.0",
    "stylelint": "14.8.2",
    "stylelint-config-css-modules": "4.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "thread-loader": "3.0.3",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3",
    "webpack": "5.69.0",
    "webpackbar": "5.0.2",
    "worker-plugin": "5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --fix",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,ts,jsx,tsx}": "eslint --ext .js,.jsx,.ts,.tsx"
  },
  "jest": {
    "resetMocks": false,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@xkool|antd|@ant-design|@babel|three|(^(rc-).+)))/"
    ]
  }
}